{% extends 'base-avanzu.html.twig' %}

 
{% block page_title %} Landing {% endblock %}
 
{% block page_content %}
   
<div class="row">  
<div class="col-md-12">


    <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">Lending</h3>
              {{ include('lending/_delete_form.html.twig') }}
              <a class="btn btn-success pull-right" href="{{ path('lending_edit', {'id': lending.id}) }}"><i class="fa fa-edit"></i></a>&nbsp;
              
            </div>
  
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lending.id }}</td>
            </tr>
            <tr>
                <th>CollectedAt</th>
                <td>{{ lending.collectedAt ? lending.collectedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ReturnedAt</th>
                <td>{{ lending.returnedAt ? lending.returnedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

 </div><!-- box -->
    </div><!-- col-md-12 -->

    </div><!-- row -->
    <a href="{{ path('lending_index') }}"><i class="fa fa-arrow-left">Back to list</i></a>

    
{% endblock %}
